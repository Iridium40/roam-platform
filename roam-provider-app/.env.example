# Supabase Configuration
VITE_PUBLIC_SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Apple OAuth (Sign in with Apple)
# These credentials are configured in Supabase Dashboard → Authentication → Providers → Apple
# You'll need: Apple Developer Organization account, Services ID, Team ID, Key ID, and .p8 private key
# See APPLE_OAUTH_SETUP_GUIDE.md for complete setup instructions

# Google OAuth (if using Google sign-in)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Plaid Configuration
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_here
PLAID_ENV=sandbox

# Twilio Configuration
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
VITE_TWILIO_CONVERSATIONS_SERVICE_SID=your_conversations_service_sid_here

# Email Configuration
RESEND_API_KEY=your_resend_api_key_here

# JWT Secret (for Phase 2 tokens)
JWT_SECRET=your_jwt_secret_here

# Application Configuration
PING_MESSAGE=ping
