{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "installCommand": "cd .. && npm install && cd roam-provider-app",
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
