{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "ignoreCommand": "bash ../scripts/vercel-ignore.sh",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' data: blob:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://accounts.google.com https://apis.google.com https://maps.googleapis.com https://maps.gstatic.com https://www.gstatic.com https://js.stripe.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://accounts.google.com https://maps.googleapis.com; font-src 'self' https://fonts.gstatic.com https://maps.gstatic.com; img-src 'self' data: blob: https: https://maps.gstatic.com https://maps.googleapis.com; connect-src 'self' https: wss: data: https://maps.googleapis.com https://api.stripe.com; frame-src 'self' https://accounts.google.com https://www.google.com https://js.stripe.com https://hooks.stripe.com https://www.youtube.com https://youtube.com https://www.youtube-nocookie.com; object-src 'none'; base-uri 'self';"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/api/notifications/edge",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PATCH, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Cache-Control"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/notifications/edge",
      "destination": "/api/notifications/edge.ts"
    },
    {
      "source": "/api/bookings/status-update",
      "destination": "/api/bookings/status-update.ts"
    },
    {
      "source": "/api/twilio-conversations",
      "destination": "/api/twilio-conversations.ts"
    },
    {
      "source": "/api/onboarding/business-info",
      "destination": "/api/onboarding/business-info.ts"
    },
    {
      "source": "/api/onboarding/upload-documents",
      "destination": "/api/onboarding/upload-documents.ts"
    },
    {
      "source": "/api/onboarding/submit-application",
      "destination": "/api/onboarding/submit-application.ts"
    },
    {
      "source": "/api/onboarding/status/(.*)",
      "destination": "/api/onboarding/status/[userId].ts"
    },
    {
      "source": "/api/admin/approve-application",
      "destination": "/api/admin/approve-application.ts"
    },
    {
      "source": "/api/stripe/create-verification-session",
      "destination": "/api/stripe/create-verification-session.ts"
    },
    {
      "source": "/api/stripe/check-verification-status/(.*)",
      "destination": "/api/stripe/check-verification-status/[sessionId].ts"
    },
    {
      "source": "/api/stripe/create-connect-account",
      "destination": "/api/stripe/create-connect-account.ts"
    },
    {
      "source": "/api/plaid/create-link-token",
      "destination": "/api/plaid/create-link-token.ts"
    },
    {
